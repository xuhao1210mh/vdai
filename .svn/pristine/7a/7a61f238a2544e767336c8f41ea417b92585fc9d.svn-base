header{
    position: relative;
    top: 0;
    left: 0;
    height: 1.32rem;
    font-size: 0.32rem;
    text-align: center;
    line-height: 1.5rem;
    color: white;
    background-color: #03cbfd;
    width: 100%;
    letter-spacing: 0.01rem;
}
header>a{
    position: absolute;
    top: 0.2rem;
    left: 0;
    width: 1.5rem;
    height: 0.88rem;
    line-height: 1rem;
    text-indent: 0.4rem;
    font-size: 0.3rem;
    color: white;
}
header a img{
    position: absolute;
    top: 0.25rem;
    left: 0.1rem;
    width: 0.53rem;
    height: 0.53rem;
    margin: auto;
}