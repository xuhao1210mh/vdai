body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,textarea,p,blockquote,th,td,hr,button,article,aside,details,figcaption,figure,footer,header,hgroup,menu,nav,section{margin:0;padding:0}
article,aside,details,figcaption,figure,footer,header,hgroup,menu,nav,section { display:block; }
h1,h2,h3,h4,h5,h6{font-size:100%;font-weight:normal;}
ul,ol{list-style:none;}
strong,b{font-weight:normal;}
em,i{font-style:normal;}
table{border-spacing:0;border-collapse:collapse;}
img{border:0;vertical-align:middle;}
input,select{vertical-align:middle;font-size:100%;line-height:150%;font-family:arial,'\5FAE\8F6F\96C5\9ED1',sans-serif;-webkit-appearance:none;}
a{text-decoration:none;outline:none;hide-focus:expression(this.hideFocus=true);background-color:transparent;-webkit-tap-highlight-color:transparent;color:black}

body{font:14px/1.5 arial,"\5FAE\8F6F\96C5\9ED1",sans-serif;-webkit-text-size-adjust:none;-ms-text-size-adjust:none;}
.fl{
    float: left;
}
.fr{
    float: right;
}
.clearFix::after{
    content: '';
    display: block;
    clear: both;
    zoom:1;
    overflow: hidden;
}
.disb{
    display: block;
    height: 100%;
}
.b0{
    border: none !important;
}
.blue {
     background: #03CBFD;
 }
a {
    text-decoration: none;
    color: #000000;
}
.navbar {
    margin-bottom:0;
}
nav.navbar {
    background: #03CBFD;
    text-align: center;
    position: relative;
    height: 13.2rem;
}
nav.navbar div{
    position: absolute;
    bottom: 2rem;
    left: 2rem;
}
